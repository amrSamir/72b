<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
 
   <servlet>
    <servlet-name>loginService</servlet-name>
    <servlet-class>com.OJToolkit_2.server.LoginServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>loginService</servlet-name>
    <url-pattern>/ojtoolkit_2/login</url-pattern>
  </servlet-mapping>
  
      <servlet>
    <servlet-name>CoderService</servlet-name>
    <servlet-class>com.OJToolkit_2.server.CoderServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>CoderService</servlet-name>
    <url-pattern>/ojtoolkit_2/coder</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>OJToolkit_2.html</welcome-file>
  </welcome-file-list>

</web-app>
